{
  "expo": {
    "name": "PrismaNote",
    "slug": "PrismaNote",
    "scheme": "prisma-note",
    "owner": "joaboateng9",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/finallogo.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.joaboateng9.prisma-note",
      "usesAppleSignIn": true,
      "splash": {
        "image": "./assets/images/finallogo.png",
        "resizeMode": "cover",
        "backgroundColor": "#000000",
        "tabletImage": "./assets/images/finallogo.png"
      },
      "notification": {
        "icon": "./assets/images/logo.png"
      }
    },
    "android": {
      "package": "com.joaboateng9.prismanote",
      "icon": "./assets/images/logo.png",
      "permissions": [
        "INTERNET",
        "WAKE_LOCK",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ],
      "usesCleartextTraffic": false,
      "splash": {
        "image": "./assets/images/finallogo.png",
        "resizeMode": "cover",
        "backgroundColor": "#000000",
        "mdpi": "./assets/images/finallogo.png",
        "hdpi": "./assets/images/finallogo.png",
        "xhdpi": "./assets/images/finallogo.png",
        "xxhdpi": "./assets/images/finallogo.png",
        "xxxhdpi": "./assets/images/finallogo.png"
      },
      "notification": {
        "icon": "./assets/images/logo.png",
        "color": "#0ea5e9"
      }
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#0ea5e9",
          "defaultChannel": "event-reminders"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "31acb497-fdfe-4d9b-8da4-0cfe11f23f59"
      }
    }
  }
}
